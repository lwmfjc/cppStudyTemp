#include "feline.h"

Feline::Feline()
{
    std::cout << "Constructor be called!(Feline)" << std::endl;
}
Feline::Feline(std::string_view fur_style, std::string_view description)
    : Animal(description), m_fur_style(fur_style)
{
    std::cout << "Constructor be called!(Feline)" << std::endl;
}

Feline::~Feline()
{
    std::cout << "Destructor be called!(Feline)" << std::endl;
}
